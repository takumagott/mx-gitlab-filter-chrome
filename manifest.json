{
	"name": "Gitlab Discussion Filter",
	"version": "0.1",
	"description": "Filter Gitlab discussions",
	"permissions": ["activeTab", "storage", ""],
	"web_accessible_resources": ["gitlab-filter.css"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/mx-beta16.png",
			"32": "images/mx-beta32.png",
			"48": "images/mx-beta48.png",
			"128": "images/mx-beta128.png"
		}
	},
	"icons": {
		"16": "images/mx-beta16.png",
		"32": "images/mx-beta32.png",
		"48": "images/mx-beta48.png",
		"128": "images/mx-beta128.png"
	},
	"manifest_version": 2
}
